{
  "parser": "@typescript-eslint/parser",
  "ignorePatterns": [
    "node_modules",
    "build"
  ],
  "plugins": [
    "import"
  ],
  "rules": {
    "no-console": [
      "warn"
    ],
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "type",
          "index",
          "object",
          "parent",
          "sibling",
          "internal"
        ]
      }
    ],
    "indent": [
      "error",
      2
    ],
    "no-duplicate-imports": [
      "error"
    ],
    "quotes": [
      "error",
      "double"
    ]
  }
}